<% company.errors.full_messages.each do |message| %>
   <p><%= message %></p> 
<% end %>
<%= form_with model: company, local: true do |f|%>
  <div>
      <%= f.label :name%>
      <%= f.text_field :name%>
  </div>
  <div>
      <%= f.label :corporate_name %>
      <%= f.text_field :corporate_name %>
  </div>
  <div>
      <%= f.label :cnpj %>
      <%= f.text_field :cnpj %>
  </div>
  <div>
      <%= f.label :address %>
      <%= f.text_field :address %>
  </div>
  <div>
      <%= f.label :social_networks%>
      <%= f.text_field :social_networks%>
  </div>
  <div>
      <%= f.label :domain%>
      <%= f.text_field :domain%>  
  </div>
  <div>
      <%= f.submit t('actions.send') %>
  </div>
<% end %>